{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bsv",
      "camelcase": "Bsv",
      "title": "BluespecSystemVerilog",
      "scope": "source.bsv",
      "file-types": [
        "bsv"
      ],
      "injection-regex": "^bsv$",
      "class-name": "TreeSitterBsv"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter parser for Bluespec SystemVerilog",
    "authors": [
      {
        "name": "Alex Chau"
      }
    ],
    "links": {
      "repository": "https://github.com/AxterDoesCode/tree-sitter-BluespecSystemVerilog"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}